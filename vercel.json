{
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "build/public/static/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/build/server.js"
    }
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=1000"
        }
      ]
    }
  ]
}
