{
  "name": "listing-frontend",
  "version": "0.1.3",
  "description": "NDLA Utlisting",
  "scripts": {
    "pretest": "npm run lint",
    "clean": "rimraf ./htdocs/assets/*",
    "test": "cross-env NODE_ENV=unittest jest",
    "tdd": "cross-env NODE_ENV=unittest jest --watch",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.prod.js --bail",
    "start": "cross-env NODE_ENV=development DISABLE_SSR=true nodemon server --watch server",
    "start-with-ssr": "cross-env NODE_ENV=development nodemon server --watch server",
    "start-prod": "cross-env NODE_ENV=production DISABLE_SSR=true node server",
    "lint": "eslint --ext .js --ext .jsx src server",
    "format": "node prettier.js write",
    "format-check": "node prettier.js lint",
    "check-all": "yarn format-check && yarn lint && yarn test"
  },
  "jest": {
    "testRegex": "/__tests__/.*-test.(js|jsx)$",
    "setupFiles": [
      "./src/__tests__/_initTestEnv.js"
    ]
  },
  "engines": {
    "node": ">=8.9.0",
    "npm": ">=5.0.0"
  },
