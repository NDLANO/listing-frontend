    "apollo": "^2.32.8",
    "babel-preset-razzle": "4.0.2",
    "chalk": "^2.4.2",
    "concurrently": "^5.3.0",
    "cross-env": "^5.1.0",
    "eslint": "^7.22.0",
    "eslint-config-ndla": "^2.0.3",
    "eslint-plugin-emotion": "^10.0.14",
    "eslint-plugin-react": "^7.21.5",
    "html-webpack-plugin": "^4.5.2",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss": "8.2.13",
    "postcss-cssnext": "^2.11.0",
    "postcss-focus": "^2.0.0",
