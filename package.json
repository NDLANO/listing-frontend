{
  "name": "listing-frontend",
  "version": "0.1.3",
  "description": "NDLA Utlisting",
  "scripts": {
    "pretest": "npm run lint",
    "clean": "rimraf ./build/*",
    "test": "cross-env BABEL_ENV=test NODE_ENV=unittest jest",
    "tdd": "cross-env NODE_ENV=unittest jest --watch",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production ts-node /scripts/build",
    "start:tsc": "tsc -b -w --preserveWatchOutput",
    "start": "cross-env NODE_ENV=development concurrently \"yarn start:tsc\" \"ts-node scripts/start-ssr\"",
    "start-without-ssr": "cross-env DISABLE_SSR=true yarn start",
    "start-prod": "cross-env NODE_ENV=production node build/server | bunyan",
    "start-prod-no-ssr": "cross-env NODE_ENV=production DISABLE_SSR=true node build/server | bunyan",
    "start-with-local-graphql": "cross-env LOCAL_GRAPHQL_API=true yarn start",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "format": "node prettier.js write",
    "format-check": "node prettier.js lint",
    "check-all": "yarn format-check && yarn lint && yarn test",
    "get-and-generate-gql-types": "graphql-codegen --config codegen.yml && prettier --write src/graphqlTypes.ts src/schema.graphql",
    "generate-gql-types-local": "graphql-codegen --config local-codegen.yml && prettier --write src/graphqlTypes.ts src/schema.graphql",
    "check-translations": "cross-env BABEL_ENV=test NODE_ENV=unittest jest src/phrases/__tests__/"
  },
  "jest": {
    "testRegex": "/__tests__/.*-test.(js|jsx|ts|tsx)$",
    "setupFiles": [
      "./src/__tests__/_initTestEnv.js"
    ]
  },
  "engines": {
    "node": ">=16.17.0",
    "npm": ">=6.14.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@emotion/eslint-plugin": "11.10.0",
    "@graphql-codegen/cli": "2.12.0",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@ndla/scripts": "^1.0.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.191",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@types/remarkable": "^2.0.3",
