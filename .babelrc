// Config is only used on the server (i.e node). Babel config for transpiling client code is located in webpack config
{
  "presets": [
    "@babel/preset-react",
    [
      "@babel/preset-env",
      { "modules": "auto", "useBuiltIns": "usage", "corejs": 2 }
    ]
  ],
  "plugins": ["@babel/plugin-proposal-object-rest-spread"]
}
